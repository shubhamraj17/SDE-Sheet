#include<bits/stdc++.h>
using namespace std;

void sumOfTwo(int arr[],int n,int target){
    unordered_map<int,int> map;
    vector<int> res;
    for(int i=0;i<n;i++){
        if(map.find(target-arr[i])!= map.end()){
            res.push_back(i);
            res.push_back(map[target-arr[i]]);
        }
        map[arr[i]]=i;
    }
    
    for(int i=0;i<res.size();i++){
        cout<<res[i]<<" ";
    }
}

int main(){
    
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    int target;
    cin>>target;
    
    sumOfTwo(arr,n,target);
}
